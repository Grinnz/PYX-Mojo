<html>
<head>
<title>Game Test</title>
</head>
<body>
<input data-bind="click: startGame, enable: canStartGame" type="submit" value="Start Game">
<form data-bind="submit: sendChat"><input data-bind="value: chatInput"></form>
<div data-bind="foreach: chatLog">
	<p>
		<span data-bind="text: timestamp"></span>
		<span data-bind="text: message"></span>
	</p>
</div>
<div id="ws_url" style="display:none"><%= url_for('ws_join')->to_abs->path("/game/$id/join") %></div>
<script src="/js/knockout.js"></script>
<script src="/js/game.js"></script>
</body>
</html>
